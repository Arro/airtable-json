{
  "name": "airtable-json",
  "version": "1.2.7",
  "description": "get airtable records, but just in raw json form",
  "author": "Phil Deschaine",
  "license": "MIT",
  "main": "dist/airtable-json.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arro/airtable-json.git"
  },
  "scripts": {
    "build": "npx babel src --out-dir dist",
    "test": "npx nyc ava"
  },
  "dependencies": {
    "airtable": "^0.9.0",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime": "^7.11.2",
    "ava": "^3.11.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.6.0",
    "eslint-plugin-babel": "^5.3.1",
    "esm": "^3.2.25",
    "nock": "^13.0.3",
    "nyc": "^15.0.1"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false,
        "configFile": false,
        "presets": [
          [
            "@babel/preset-env",
            {
              "useBuiltIns": "entry",
              "corejs": 3
            }
          ]
        ],
        "plugins": [
          [
            "@babel/plugin-transform-runtime",
            {
              "regenerator": true
            }
          ]
        ]
      }
    },
    "files": [
      "test/populate-many.js",
      "test/main.js",
      "test/populate.js",
      "test/filter.js"
    ],
    "require": [
      "esm"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/airtable-json.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "engines": {
    "node": ">=13.5.0"
  }
}

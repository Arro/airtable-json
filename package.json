{
  "name": "airtable-json",
  "version": "1.3.1",
  "description": "get airtable records, but just in raw json form",
  "author": "Phil Deschaine",
  "license": "MIT",
  "main": "dist/airtable-json.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arro/airtable-json.git"
  },
  "scripts": {
    "build": "npx babel src --out-dir dist",
    "test": "npx nyc ava"
  },
  "dependencies": {
    "airtable": "^0.10.1",
    "core-js": "^3.9.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/eslint-parser": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/register": "^7.13.8",
    "@babel/runtime": "^7.13.9",
    "ava": "^3.15.0",
    "eslint": "^7.21.0",
    "nock": "^13.0.10",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1"
  },
  "ava": {
    "files": [
      "test/populate-many.js",
      "test/main.js",
      "test/populate.js",
      "test/filter.js"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/airtable-json.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
